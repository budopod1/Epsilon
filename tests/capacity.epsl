Z#main {
    [Z]:arr = [Z]*0;

    if ([arr].capacity == 0) {
        return 0;
    };

    [arr].require_capacity[25];

    if ([arr].capacity != 25) {
        return 0;
    };

    [arr].append[2];
    [arr].extend[[Z] [5, 6]];

    if ([arr].capacity != 25) {
        return 0;
    };

    [arr].shrink_mem;

    if ([arr].capacity != 3) {
        return 0;
    };

    return 1;
}
