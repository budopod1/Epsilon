Z#main {
    [Z]:nums = [Z] [0, 1, 1, 2, 3, 5, 8, 13];

    [Z]:doubled = {n * 2 | Z:n in nums};

    [Z]:even = {n | Z:n in nums, n % 2 == 0};

    [Str]:fizbuzz = {"FizzBuzz" | Z:n in nums, n % 3 == 0, n % 5 == 0};

    [Z]:nums2 = [Z] [1, 2, 3, 6, 11];

    [Z]:multiples = {n * m | Z:n in nums, Z:m in nums2};

    println[doubled];
    println[even];
    println[fizbuzz];

    return 0;
}
